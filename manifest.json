{
	"manifest_version": 2,
	"name": "Get Chromivated",
	"version": "0.1",

	"description": "Get Motivated at the click of a new tab!",
	"icons": {
		"default_icon": "icon.png"
	},
	"background": {
		"scripts": ["chromivated.js"]
	},

	"permissions": [
		"tabs"
	],

	"chrome_url_overrides" : {
	  "newtab": "chromivated.html"
	}
}